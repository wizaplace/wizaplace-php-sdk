{
    "name": "wizaplace/sdk",
    "description": "Wizaplace's PHP SDK",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.4",
        "myclabs/php-enum": "^1.5"
    },
    "autoload": {
        "psr-4": {"Wizaplace\\": "src/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "wizaplace/php-coding-standard": "^1.0",
        "m6web/coke": "^2.2",
        "phpstan/phpstan": "^0.7.0",
        "phpstan/phpstan-guzzle": "^0.7.0",
        "fr3d/swagger-assertions": "^0.8.1",
        "php-vcr/php-vcr": "^1.3",
        "symfony/yaml": "^3.2",
        "justinrainbow/json-schema": "^5.2.1"
    },
    "autoload-dev": {
        "psr-4": { "Wizaplace\\Tests\\": "tests/" }
    }
}
